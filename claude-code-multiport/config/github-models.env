# GitHub Models Service Configuration - Port 8092
# ================================================

# Service Configuration
SERVICE_PORT=8092
SERVICE_NAME=github-models
PROVIDER=github_models

# GitHub Models Configuration
GITHUB_API_KEY=your_github_pat_token_here
GITHUB_TOKEN=your_github_pat_token_here
GITHUB_MODELS_ENDPOINT=https://models.github.ai/inference

# Model Configuration
PRIMARY_MODEL=claude-3-5-sonnet
SECONDARY_MODEL=claude-3-5-haiku
FALLBACK_MODEL=gpt-4o-mini

# Service Settings
MAX_TOKENS_LIMIT=8192
RATE_LIMIT_PER_MINUTE=100
HEALTH_CHECK_INTERVAL=30
TIMEOUT_SECONDS=60

# Logging
LOG_LEVEL=INFO
LOG_FILE=/home/sam/claude-code-multimodel/logs/github-models.log

# Environment
ENVIRONMENT=production
DEBUG=false